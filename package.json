{
  "name": "esb",
  "version": "0.9.0",
  "description": "Esb desktop app",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "build": "electron-builder -m",
    "watch": "nodemon --exec 'electron .'",
    "release": "electron-builder -mwl --publish 'onTagOrDraft'"
  },
  "repository": "https://github.com/ikakuca/electronic-service-book",
  "homepage": "https://github.com/ikakuca/electronic-service-book",
  "author": "vozzi",
  "license": "ISC",
  "devDependencies": {
    "electron": "8.2.2",
    "electron-builder": "^22.4.1",
    "electron-rebuild": "^1.11.0",
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "electron-log": "^4.2.0",
    "electron-updater": "^4.3.1",
    "smartcard": "^1.0.32"
  },
  "build": {
    "appId": "com.vozzi.esb",
    "copyright": "Copyright 2020 KOV Technology doo",
    "publish": {
      "provider": "github"
    },
    "mac": {
      "category": "public.app-category.utilities",
      "target": "dmg"
    },
    "win": {
      "target": "zip",
      "certificateFile": "private/certificate.p12",
      "certificatePassword": "",
      "verifyUpdateCodeSignature": false
    },
    "linux": {
      "category": "Utility"
    }
  }
}
